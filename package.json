{
    "name": "z23",
    "description": "A Ghost Handlebars theme for art galleries",
    "version": "0.1.0",
    "license": "MIT",
    "engines": {
        "ghost": ">=5.44.0"
    },
    "demo": "https://zawyeh.dev/",
    "keywords": [
        "ghost",
        "theme",
        "ghost-theme",
        "responsive",
        "mobile-friendly"
    ],
    "author": {
        "name": "Salaheddin AbuEin",
        "email": "salah@zawyeh.dev",
        "url": "https://sabuein.github.io/"
    },
    "screenshots": {
        "desktop": "assets/screenshot-desktop.jpg",
        "mobile": "assets/screenshot-mobile.jpg"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/sabuein/z23.git"
    },
    "bugs": "https://github.com/sabuein/z23/issues/",
    "contributors": [
        "https://github.com/sabuein/z23/issues/graphs/contributors"
    ],
    "browserslist": [
        "defaults"
    ],
    "config": {
